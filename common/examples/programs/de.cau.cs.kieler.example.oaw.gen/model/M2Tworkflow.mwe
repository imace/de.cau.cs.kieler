<?xml version="1.0"?>
<!--
Require-Bundle: org.eclipse.ui,
 org.eclipse.emf.mwe.core;bundle-version="0.7.0",
 org.eclipse.emf.mwe.utils;bundle-version="0.7.0",
 org.eclipse.xpand;bundle-version="0.7.0",
 org.eclipse.emf.ecore.xmi;bundle-version="2.5.0",
 org.eclipse.xtend;bundle-version="0.7.0",
 org.eclipse.xtend.typesystem.emf;bundle-version="0.7.0",
 com.ibm.icu;bundle-version="4.0.1",
 org.antlr.runtime;bundle-version="3.0.0" 
 -->
 
<workflow>
	<property name="metamodel" value="platform:/resource/de.cau.cs.kieler.example.oaw/model/mymodel.ecore" />
	<property name="model" value="platform:/resource/de.cau.cs.kieler.example.oaw.gen/My.mymodel" />
	<property name="src-gen" value="src-gen" />
	
	<!-- set up EMF for standalone execution -->
	<!--  <bean class="org.eclipse.mwe.emf.StandaloneSetup" >  --> 
	<bean class="org.eclipse.emf.mwe.utils.StandaloneSetup" >
		<platformUri value=".."/>
	</bean>
	 

	<!-- load model and store it in slot 'model' -->
	<!--  <component class="org.eclipse.mwe.emf.Reader"> -->
	<component class="org.eclipse.emf.mwe.utils.Reader">
        <!--   <uri value="platform:/resource/${model}" />   -->  
    	<uri value="${model}" /> 
		<!-- <uri value="platform:/resource/${model}" />  -->
		<modelSlot value="model" />
	</component>

	<!--  generate code <metaModelFile value="${metamodel}" /> -->
	<!--  <component class="org.openarchitectureware.xpand2.Generator">  -->
	<component class="org.eclipse.xpand2.Generator">
	<!-- <metaModel class="oaw.type.emf.EmfMetaModel"> -->
	<metaModel class="org.eclipse.xtend.typesystem.emf.EmfRegistryMetaModel">
		<!-- <metaModelFile value="${metamodel}" /> -->
	</metaModel>
    <!-- <metaModel id='mm' class='org.eclipse.m2t.type.emf.EmfRegistryMetaModel' />  -->  
    <metaModel id='mm' class='org.eclipse.xtend.typesystem.emf.EmfRegistryMetaModel' />  
		 <expand
			value="model::M2Ttemplate::main2 FOR model" />
		<outlet path="${src-gen}" />
        
	</component>
</workflow>
