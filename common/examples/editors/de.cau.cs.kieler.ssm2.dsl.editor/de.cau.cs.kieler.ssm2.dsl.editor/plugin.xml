<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.cau.cs.kieler.ssm2.dsl.editor.Ssm2dlEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="ssm2dl"
            icon="icons/file.gif"
            id="de.cau.cs.kieler.ssm2.dsl.editor.Ssm2dlEditor"
            name="ssm2dl Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="de.cau.cs.kieler.ssm2.dsl.wizards.NewSsm2dlProjectWizard"
			icon="icons/template.gif"
			id="de.cau.cs.kieler.ssm2.dsl.wizards.NewSsm2dlProjectWizard"
			name="ssm2dl Project"
			project="true">
		</wizard>
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="de.cau.cs.kieler.ssm2.dsl.wizards.NewSsm2dlFileWizard"
			icon="icons/file.gif"
			id="de.cau.cs.kieler.ssm2.dsl.wizards.NewSsm2dlFileWizard"
			name="ssm2dl Editor file">
		</wizard>
	</extension>
</plugin>
