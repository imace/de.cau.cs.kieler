<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.statusHandlers">
      <statusHandler
            class="de.cau.cs.kieler.core.ui.handler.DebugErrorHandler"
            id="de.cau.cs.kieler.core.ui.statusHandler">
      </statusHandler>
      <statusHandlerProductBinding
            handlerId="de.cau.cs.kieler.core.ui.statusHandler"
            productId="de.cau.cs.kieler.core.ui.kielerproduct">
      </statusHandlerProductBinding>
   </extension>
   <extension
         id="KielerProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="KIELER - Kiel Integrated Environment for Layout for the Eclipse RCP">
         <property
               name="windowImages"
               value="eclipse.gif,eclipse32.gif,eclipse48.gif,eclipse.png,eclipse32.png">
         </property>
         <property
               name="aboutText"
               value="%productBlurb">
         </property>
         <property
               name="aboutImage"
               value="eclipse_lg.gif">
         </property>
         <property
               name="startupForegroundColor"
               value="C8D5EA">
         </property>
         <property
               name="startupProgressRect"
               value="0,280,455,15">
         </property>
         <property
               name="startupMessageRect"
               value="7,220,441,20">
         </property>
      </product>
   </extension>
   <extension
         id="kielerproduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="KIELER - Kiel Integrated Environment for Layout for the Eclipse RCP">
         <property
               name="windowImages"
               value="eclipse.gif,eclipse32.gif,eclipse48.gif,eclipse.png,eclipse32.png">
         </property>
         <property
               name="aboutText"
               value="%productBlurb">
         </property>
         <property
               name="aboutImage"
               value="eclipse_lg.gif">
         </property>
         <property
               name="startupForegroundColor"
               value="C8D5EA">
         </property>
         <property
               name="startupProgressRect"
               value="0,280,455,15">
         </property>
         <property
               name="startupMessageRect"
               value="7,220,441,20">
         </property>
         <property
               name="appName"
               value="KIELER - Kiel Integrated Environment for Layout for the Eclipse RCP">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
