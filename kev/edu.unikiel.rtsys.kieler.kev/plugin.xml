<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="KIELER"
            id="edu.unikiel.rtsys.kieler.kev">
      </category>
      <view
            name="Kiel Environment View"
            icon="icons/sample.gif"
            category="edu.unikiel.rtsys.kieler.kev"
            class="edu.unikiel.rtsys.kieler.kev.views.EnvironmentView"
            id="edu.unikiel.rtsys.kieler.kev.views.EnvironmentView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.TaskList"
               relationship="right"
               id="edu.unikiel.rtsys.kieler.kev.views.EnvironmentView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
