module ifModule:
input A,B,C;

	if A then emit A
	elsif B 
		then emit B
	elsif C 
		then emit C
	elsif A and B
		% in case no statement is specified, 'nothing' is inserted!
	else 
		nothing 
	end if
	||
	if A then 
		nothing
	end if
	||
	if A then 
		emit A
	else 
		nothing
	end if	
end module