        <annotations xsi:type="ann:TypedStringAnnotation" name="text" value="The SensorHandler simulates the interrupt service handling routine associated with the&#xA;sensor. executionTime parameter annotated at the input port of the SensorHandler&#xA;is used to simulate the execution time of the interrupt service routine." type="ptolemy.kernel.util.StringAttribute"/>
        <annotations xsi:type="ann:TypedStringAnnotation" name="_location" value="{-635.0, -80.0}" type="ptolemy.kernel.util.Location"/>
        <annotations xsi:type="ann:ReferenceAnnotation" name="attachedTo"/>
