                        <outgoingTransitions priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.11"/>
                      </innerStates>
                      <innerStates id="S0" label="cs_jam">
                        <outgoingTransitions triggersAndEffects="/ EMERGENCY" priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.9">
                          <effects xsi:type="synccharts:Emission" signal="EMERGENCY"/>
                        </outgoingTransitions>
                      </innerStates>
                      <innerStates id="S1" label="breakage_a_shaft_ppum" isFinal="true"/>
                      <innerStates id="S2" label="breakage_b_shaft_ppum" isFinal="true"/>
                      <innerStates id="S3" label="failure_controller_a" isFinal="true"/>
                    </regions>
                    <outgoingTransitions triggersAndEffects="/ ONEMOT1" type="NORMALTERMINATION" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3">
                      <effects xsi:type="synccharts:Emission" signal="ONEMOT1"/>
                    </outgoingTransitions>
