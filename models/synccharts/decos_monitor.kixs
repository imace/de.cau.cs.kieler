                      <innerStates id="S0" label="failure_controller_b" isFinal="true"/>
                      <innerStates id="S1" label="break_shaft_b_or_jam" isFinal="true"/>
                      <innerStates id="S2" label="break_a_or_cs_or_jam" isFinal="true"/>
                      <innerStates id="S3" label="break_b_or_cs_or_jam" isFinal="true"/>
