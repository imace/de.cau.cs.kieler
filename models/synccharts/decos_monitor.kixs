                          <effects xsi:type="synccharts:Emission" signal="EMERGENCY"/>
                        </outgoingTransitions>
                      </innerStates>
                      <innerStates id="C2" label="C2">
                        <outgoingTransitions targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.6"/>
                        <outgoingTransitions priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.7"/>
                      </innerStates>
                      <innerStates id="S0" label="breakage_a_shaft_ppura" isFinal="true"/>
                      <innerStates id="S1" label="breakage_b_shaft_ppura" isFinal="true"/>
                      <innerStates id="C3" label="C3">
                        <outgoingTransitions priority="2" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.9"/>
