                        <outgoingTransitions triggersAndEffects="TIMER / EMERGENCY" priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3/@regions.0/@innerStates.0">
                          <effects xsi:type="synccharts:Emission" signal="EMERGENCY"/>
                          <trigger xsi:type="synccharts:SignalReference" signal="TIMER"/>
                        </outgoingTransitions>
                        <outgoingTransitions triggersAndEffects="EXCEED" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3/@regions.0/@innerStates.1">
                          <trigger xsi:type="synccharts:SignalReference" signal="EXCEED"/>
                        </outgoingTransitions>
                      </innerStates>
                      <innerStates id="C1" label="C1">
                        <outgoingTransitions targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3/@regions.0/@innerStates.2"/>
                        <outgoingTransitions priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3/@regions.0/@innerStates.5"/>
                      </innerStates>
                      <innerStates id="C2" label="C2">
                        <outgoingTransitions targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3/@regions.0/@innerStates.3"/>
                        <outgoingTransitions priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3/@regions.0/@innerStates.4"/>
