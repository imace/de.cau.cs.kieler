                  <innerStates id="S" label="init" isInitial="true">
                    <outgoingTransitions triggersAndEffects="TIMER / DRV1_RDY" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.1">
                      <effects xsi:type="synccharts:Emission" signal="DRV1_RDY"/>
                      <trigger xsi:type="synccharts:SignalReference" signal="TIMER"/>
                    </outgoingTransitions>
                  </innerStates>
                  <innerStates id="C1" label="C1">
                    <outgoingTransitions triggersAndEffects="/ TWOMOT1" priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2">
                      <effects xsi:type="synccharts:Emission" signal="TWOMOT1"/>
                    </outgoingTransitions>
                    <outgoingTransitions triggersAndEffects="FAIL_ACTUATOR / ONEMOT1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.3">
                      <effects xsi:type="synccharts:Emission" signal="ONEMOT1"/>
                      <trigger xsi:type="synccharts:SignalReference" signal="FAIL_ACTUATOR"/>
                    </outgoingTransitions>
                  </innerStates>
                  <innerStates id="S0" label="two_motors">
                    <regions>
                      <innerStates id="S" label="monitor" isInitial="true">
                        <outgoingTransitions triggersAndEffects="pre (FAIL_ACTUATOR)" priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.1">
                          <trigger xsi:type="synccharts:ComplexExpression" operator="pre">
                            <subExpressions xsi:type="synccharts:SignalReference" signal="FAIL_ACTUATOR"/>
                          </trigger>
                        </outgoingTransitions>
                        <outgoingTransitions triggersAndEffects="EXCEED" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.2">
                          <trigger xsi:type="synccharts:SignalReference" signal="EXCEED"/>
                        </outgoingTransitions>
                      </innerStates>
                      <innerStates id="S0" label="ppu_ace_error" isFinal="true"/>
                      <innerStates id="C1" label="C1">
                        <outgoingTransitions priority="1" targetState="//@innerStates.0/@regions.1/@innerStates.1/@regions.0/@innerStates.0/@regions.0/@innerStates.1/@regions.0/@innerStates.2/@regions.0/@innerStates.3"/>
