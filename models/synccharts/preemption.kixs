<?xml version="1.0" encoding="UTF-8"?>
<synccharts:Region xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:expressions="http://kieler.cs.cau.de/expressions/0.1.2" xmlns:synccharts="http://kieler.cs.cau.de/synccharts/0.2.3">
  <states id="SyncChart" label="SyncChart">
    <signals name="power" isInput="true"/>
    <signals name="running" isOutput="true"/>
    <regions>
      <states id="off" label="off" isInitial="true" incomingTransitions="//@states.0/@regions.0/@states.1/@outgoingTransitions.0">
        <outgoingTransitions label="power" priority="1" targetState="//@states.0/@regions.0/@states.1">
          <trigger xsi:type="expressions:ValuedObjectReference" valuedObject="//@states.0/@signals.0"/>
        </outgoingTransitions>
      </states>
      <states id="on" label="on" incomingTransitions="//@states.0/@regions.0/@states.0/@outgoingTransitions.0">
        <regions>
          <states id="sustain" label="sustain" isInitial="true" incomingTransitions="//@states.0/@regions.0/@states.1/@regions.0/@states.0/@outgoingTransitions.0">
            <outgoingTransitions label="/ running" priority="1" targetState="//@states.0/@regions.0/@states.1/@regions.0/@states.0">
              <effects xsi:type="synccharts:Emission" signal="//@states.0/@signals.1"/>
            </outgoingTransitions>
          </states>
        </regions>
        <outgoingTransitions label="power" priority="1" type="STRONGABORT" targetState="//@states.0/@regions.0/@states.0">
          <trigger xsi:type="expressions:ValuedObjectReference" valuedObject="//@states.0/@signals.0"/>
        </outgoingTransitions>
      </states>
    </regions>
  </states>
</synccharts:Region>
