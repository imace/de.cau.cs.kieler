<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="de.cau.cs.kieler.ui.export"
            name="KIELER">
      </category>
      <wizard
            category="de.cau.cs.kieler.ui.export"
            class="de.cau.cs.kieler.kex.ui.wizards.exporting.ExampleExportWizard"
            icon="icons/keksIcon.gif"
            id="de.cau.cs.kieler.kex.ui.exportWizard"
            name="Example">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
         
      <wizard
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.kex.ui.wizards.importing.ExampleImportWizard"
            icon="icons/keksIcon.gif"
            id="de.cau.cs.kieler.kex.ui.importWizard"
            name="Example">
      </wizard>
   </extension>
 <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="de.cau.cs.kieler.examples"
            name="KIELER"
            parentCategory="org.eclipse.ui.Examples">
      </category>
       </extension>
    <extension point="org.eclipse.ui.newWizards">
        <wizard    id="de.cau.cs.kieler.kex.ui.newWizard"
            name="KIELER Examples"
            class="de.cau.cs.kieler.kex.ui.wizards.importing.ExampleImportWizard"
            category="org.eclipse.ui.Examples/de.cau.cs.kieler.examples"
            icon="icons/keksIcon.gif"
            project="true">
            <description>%DomainExample_description</description>
        </wizard>
    </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/introContent.xml">
      </configExtension>
   </extension>
</plugin>
