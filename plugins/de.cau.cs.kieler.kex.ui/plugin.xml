<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="de.cau.cs.kieler.kex.ui.actionSet"
            label="KEX"
            visible="true">
         <menu
               id="kexMenu"
               label="KEX">
            <separator
                  name="kexGroup">
            </separator>
         </menu>
         <action
               class="de.cau.cs.kieler.kex.ui.actions.KEXImportAction"
               id="de.cau.cs.kieler.kex.ui.actions.import"
               label="KIELER Example Import"
               menubarPath="kexMenu/kexGroup"
               style="push"
               tooltip="Import examples to your local workspace">
         </action>
         <action
               class="de.cau.cs.kieler.kex.ui.actions.KEXExportAction"
               id="de.cau.cs.kieler.kex.ui.export"
               label="KIELER Example Export"
               menubarPath="kexMenu/kexGroup"
               style="push"
               tooltip="Export examples of your local workspace">
         </action>
      </actionSet>
   </extension>
</plugin>
