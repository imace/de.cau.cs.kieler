<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension
      point="org.eclipse.graphiti.ui.diagramTypes">
    <diagramType
      description="This is the diagram type for my Graphiti tutorial"
      id="de.cau.cs.kieler.rail.editor.DiagramType"
      name="Rail"
      type="Rail">
    </diagramType>
  </extension>
 
  <extension
      point="org.eclipse.graphiti.ui.diagramTypeProviders">
    <diagramTypeProvider
      class="de.cau.cs.kieler.rail.editor.DiagramTypeProvider"
      description="This is the editor for rail"
      id="de.cau.cs.kieler.rail.editor.DiagramTypeProvider"
      name="My tutorial editor">
      <diagramType
        id="de.cau.cs.kieler.rail.editor.DiagramType">
      </diagramType>
    </diagramTypeProvider>
  </extension>
  <extension
        point="org.eclipse.ui.newWizards">
     <wizard
           class="de.cau.cs.kieler.KrailCreationWizardPage"
           id="de.cau.cs.kieler.rail.editor.WizardPage"
           name="Rail editor">
     </wizard>
  </extension>
  <extension
        point="org.eclipse.ui.editors">
     <editor
           default="false"
           extensions="krail"
           id="de.cau.cs.kieler.rail.editor"
           name="Rail Editor">
     </editor>
  </extension>
</plugin>
