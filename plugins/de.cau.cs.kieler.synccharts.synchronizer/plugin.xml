<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.cau.cs.kieler.synccharts.synchronizer.ModelSynchronizer">
      </startup>
   </extension>
   <extension
         point="de.cau.cs.kieler.synccharts.triggerListener">
      <triggerListener
            class="de.cau.cs.kieler.synccharts.synchronizer.ModelChangeTriggerListener">
      </triggerListener>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            category="de.cau.cs.kieler"
            categoryId="de.cau.cs.kieler"
            defaultHandler="de.cau.cs.kieler.synccharts.synchronizer.SyncroActivationHandler"
            id="de.cau.cs.kieler.synccharts.synchronizer.Activate"
            name="ActiveSynchronization">
         <state
               class="org.eclipse.ui.handlers.RegistryToggleState:false"
               id="org.eclipse.ui.commands.toggleState">
         </state>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:de.cau.cs.kieler?after=de.cau.cs.kieler.kiml.ui.command.layout">
         <command
               commandId="de.cau.cs.kieler.synccharts.synchronizer.Activate"
               icon="icons/refresh.gif"
               label="ActivateSynchronization"
               style="toggle">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.emf.compare.diff.mergerprovider">
      <mergerprovider
            fileExtension="kixs"
            mergerProviderClass="de.cau.cs.kieler.synccharts.synchronizer.merge.KitsMergerProvider"
            priority="normal">
      </mergerprovider>
      <mergerprovider
            fileExtension="kits"
            mergerProviderClass="de.cau.cs.kieler.synccharts.synchronizer.merge.KitsMergerProvider"
            priority="normal">
      </mergerprovider>
   </extension>
   <extension
         point="org.eclipse.emf.compare.match.engine">
      <matchengine
            engineClass="de.cau.cs.kieler.synccharts.synchronizer.match.KitsMatchEngine"
            fileExtension="kits"
            label="de.cau.cs.kieler.synccharts.synchronizer.match.KitsMatchEngine"
            priority="normal">
      </matchengine>
      <matchengine
            engineClass="de.cau.cs.kieler.synccharts.synchronizer.match.KitsMatchEngine"
            fileExtension="kixs"
            label="de.cau.cs.kieler.synccharts.synchronizer.match.KitsMatchEngine"
            priority="normal">
      </matchengine>
   </extension>
   <extension
         point="org.eclipse.emf.compare.diff.engine">
      <diffengine
            engineClass="de.cau.cs.kieler.synccharts.synchronizer.diff.KitsDiffEngine"
            fileExtension="kits"
            label="de.cau.cs.kieler.synccharts.synchronizer.diff.KitsDiffEngine"
            priority="normal">
      </diffengine>
      <diffengine
            engineClass="de.cau.cs.kieler.synccharts.synchronizer.diff.KitsDiffEngine"
            fileExtension="kixs"
            label="de.cau.cs.kieler.synccharts.synchronizer.diff.KitsDiffEngine"
            priority="normal">
      </diffengine>
   </extension>

</plugin>
