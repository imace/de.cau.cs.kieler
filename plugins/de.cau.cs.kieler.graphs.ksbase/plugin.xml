       </transformationCommand>
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.toggleNodeHyper">
       </transformationCommand>
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.encapsulate">
       </transformationCommand>
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.addCircle">
       </transformationCommand>
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.addClique">
       </transformationCommand>
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.addTree">
       </transformationCommand>
    </menuContribution>
    <menuContribution
          locationURI="popupBar:any">
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.addSuc">
       </transformationCommand>
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.addPred">
       </transformationCommand>
       <transformationCommand
             transformationId="de.cau.cs.kieler.graphs.ksbase.addNeigh">
