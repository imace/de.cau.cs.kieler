<plugin>
   <extension
         point="de.cau.cs.kieler.sim.kiem.json.datacomponent">
      <content
            class="de.cau.cs.kieler.uml2.sim.kiem.DataComponent"
            id="de.cau.cs.kieler.uml2.sim"
            name="UML2 Maude Simulator">
      </content>
   </extension>
   <extension
         point="de.cau.cs.kieler.sim.kiem.config.DefaultScheduleContributor">
      <executionFile
            file="execution/uml_maude.execution">
      </executionFile>
   </extension>
   <extension
         point="de.cau.cs.kieler.core.model.Validation">
      <ePackageDefinition
            ePackageClass="org.eclipse.uml2.uml.UMLPackage"
            ePackageId="org.eclipse.uml2.uml"
            ePackageNsURI="http://www.eclipse.org/uml2/3.0.0/UML"
            ePackageNsURIDiagram="http://www.eclipse.org/gmf/runtime/1.0.2/notation"
            ePackageNsURIModel="http://www.eclipse.org/uml2/3.0.0/UML">
      </ePackageDefinition>
      <Checkfile
            class="de.cau.cs.kieler.uml2.sim.maude.validation.ModelDiagramInterface"
            ePackageId="org.eclipse.uml2.uml"
            editorClassName="org.eclipse.papyrus.editor.PapyrusMultiDiagramEditor"
            id="de.cau.cs.kieler.uml2.UMLMaudeChecks"
            isEnabledByDefault="true"
            isWrapExistingValidator="true"
            name="Maude Checks"
            path="model/UMLMaudeChecks.chk"
            tooltip="Check whether the UML model can be simulated with Maude.">
         <referenceEPackageNsURI
               nsURI="http://www.eclipse.org/uml2/3.0.0/UML">
         </referenceEPackageNsURI>
         <referenceEPackageNsURI
               nsURI="http://www.eclipse.org/gmf/runtime/1.0.2/notation">
         </referenceEPackageNsURI>
      </Checkfile>
      <ValidateAction
            actionFactory="de.cau.cs.kieler.uml2.sim.maude.validation.PapyrusValidationActionFactory"
            ePackageId="org.eclipse.uml2.uml">
      </ValidateAction>
   </extension>



</plugin>
