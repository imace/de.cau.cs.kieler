<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            canFinishEarly="false"
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.dataflow.ui.wizards.CreateRandomModelWizard"
            hasPages="true"
            icon="icons/new_model.gif"
            id="de.cau.cs.kieler.dataflow.ui.wizards.CreateRandomModelWizard"
            name="Create Random Dataflow Model"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.cau.cs.kieler.dataflow.ui.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonWizard
            menuGroupId="de.cau.cs.kieler"
            type="new"
            wizardId="de.cau.cs.kieler.dataflow.ui.wizards.CreateRandomModelWizard">
         <enablement>
            <or>
               <adapt
                     type="org.eclipse.core.resources.IProject">
               </adapt>
               <adapt
                     type="org.eclipse.core.resources.IFolder">
               </adapt>
            </or>
         </enablement>
      </commonWizard>
   </extension>

</plugin>
