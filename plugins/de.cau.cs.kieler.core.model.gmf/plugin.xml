<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cau.cs.kieler.core.model.graphicalFrameworkBridges">
      <bridge
            class="de.cau.cs.kieler.core.model.gmf.GmfFrameworkBridge"
            id="gmf">
      </bridge>
   </extension>
   <extension
         point="de.cau.cs.kieler.core.kivi.combinations">
      <combination
            active="true"
            class="de.cau.cs.kieler.core.model.gmf.combinations.ShowHierarchyCombination"
            description="Show the hierarchy levels of a diagram by painting each level in a different color."
            name="Show Hierarchy Levels">
      </combination>
   </extension>

</plugin>
