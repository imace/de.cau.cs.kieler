   <element name="configuration">
      <annotation>
         <documentation>
            Configures structure based editing features for an editor
         </documentation>
      </annotation>
      <complexType>
         <sequence>
            <element ref="menus"/>
            <element ref="transformations"/>
            <element ref="parameterDiagramMapping"/>
         </sequence>
         <attribute name="editor" type="string" use="required">
