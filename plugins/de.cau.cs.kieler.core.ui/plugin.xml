         point="org.eclipse.ui.intro.config">
      <config
            content="help/introContent.xml"
            id="de.cau.cs.kieler.intro.config"
            introId="org.eclipse.ui.intro.universal">
         <presentation
               home-page-id="root">
         </presentation>
      </config>
   </extension>
-->
</plugin>
