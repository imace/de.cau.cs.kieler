<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="de.cau.cs.kieler.uml2.layout.class"
         name="Class Diagram Layout"
         point="de.cau.cs.kieler.kiml.layoutInfo">
      <option
            class="org.eclipse.uml2.diagram.clazz.edit.parts.PackageEditPart"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.classDiagram">
      </option>
      <option
            class="org.eclipse.uml2.uml.Generalization"
            option="de.cau.cs.kieler.edgeType"
            value="GENERALIZATION">
      </option>
      <option
            class="org.eclipse.uml2.uml.Association"
            option="de.cau.cs.kieler.edgeType"
            value="ASSOCIATION">
      </option>
      <option
            class="org.eclipse.uml2.uml.Dependency"
            option="de.cau.cs.kieler.edgeType"
            value="DEPENDENCY">
      </option>
   </extension>
   <extension
         id="de.cau.cs.kieler.uml2.layout.activity"
         name="Activity Diagram Layout"
         point="de.cau.cs.kieler.kiml.layoutInfo">
      <option
            class="org.eclipse.uml2.diagram.activity.edit.parts.PackageEditPart"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.boxes">
      </option>
      <option
            class="org.eclipse.uml2.uml.Activity"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.stateMachine">
      </option>
      <option
            class="org.eclipse.uml2.uml.Activity"
            option="de.cau.cs.kieler.direction"
            value="DOWN">
      </option>
      <option
            class="org.eclipse.uml2.uml.Activity"
            option="de.cau.cs.kieler.borderSpacing"
            value="20.0">
      </option>
      <option
            class="org.eclipse.uml2.uml.Activity"
            option="de.cau.cs.kieler.spacing"
            value="25.0">
      </option>
   </extension>
   <extension
         id="de.cau.cs.kieler.uml2.layout.useCase"
         name="Use Case Diagram Layout"
         point="de.cau.cs.kieler.kiml.layoutInfo">
      <option
            class="org.eclipse.uml2.diagram.usecase.edit.parts.PackageEditPart"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.usecaseDiagram">
      </option>
      <option
            class="org.eclipse.uml2.diagram.usecase.edit.parts.PackageEditPart"
            option="de.cau.cs.kieler.spacing"
            value="40.0">
      </option>
      <option
            class="org.eclipse.uml2.diagram.usecase.edit.parts.PackageEditPart"
            option="de.cau.cs.kieler.borderSpacing"
            value="20.0">
      </option>
   </extension>
   <extension
         id="de.cau.cs.kieler.uml2.layout.stateMachine"
         name="State Machine Diagram Layout"
         point="de.cau.cs.kieler.kiml.layoutInfo">
      <option
            class="org.eclipse.uml2.diagram.statemachine.edit.parts.PackageEditPart"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.boxes">
      </option>
      <option
            class="org.eclipse.uml2.uml.StateMachine"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.boxes">
      </option>
      <option
            class="org.eclipse.uml2.uml.StateMachine"
            option="de.cau.cs.kieler.spacing"
            value="1.0">
      </option>
      <option
            class="org.eclipse.uml2.uml.StateMachine"
            option="de.cau.cs.kieler.borderSpacing"
            value="1.0">
      </option>
      <option
            class="org.eclipse.uml2.uml.StateMachine"
            option="de.cau.cs.kieler.expandNodes"
            value="true">
      </option>
      <option
            class="org.eclipse.uml2.uml.Region"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.stateMachine">
      </option>
      <option
            class="org.eclipse.uml2.uml.Region"
            option="de.cau.cs.kieler.direction"
            value="RIGHT">
      </option>
   </extension>
   <extension
         point="de.cau.cs.kieler.ksbase.configurations">
      <configuration
            TransformationFile="src/activity.ext"
            checkVisibility="true"
            contextId="org.eclipse.uml2.diagram.activity.ui.diagramContext"
            editorId="org.eclipse.uml2.diagram.activity.part.UMLDiagramEditorID">
       <transformations>
          <transformation
                icon="icons/action.gif"
                keyboardShortcut="M1+R A"
                menu="true"
                name="Create Successor"
                popup="false"
                popupbar="false"
                separated="false"
                toolbar="true"
                transformation="activityCreateSuccessor"
                transformationId="de.cau.cs.kieler.uml2.activity.createSuccessor">
          </transformation>
          <transformation
                icon="icons/flow.gif"
                keyboardShortcut="M1+R F"
                menu="true"
                name="Create Control Flow"
                popup="false"
                popupbar="false"
                separated="false"
                toolbar="true"
                transformation="activityControlFlow"
                transformationId="de.cau.cs.kieler.uml2.activity.controlFlow">
          </transformation>
          <transformation
                icon="icons/toggle_action.gif"
                keyboardShortcut="M1+R T"
                menu="true"
                name="Toggle Action"
                popup="false"
                popupbar="false"
                separated="false"
                toolbar="true"
                transformation="activityToggleAction"
                transformationId="de.cau.cs.kieler.uml2.activity.toggleAction">
          </transformation>
       </transformations>
       <package
             class="org.eclipse.uml2.uml.UMLPackage">
       </package>
       <XtendTransformationFile
             TransformationFile="src/activity.ext">
       </XtendTransformationFile>
      </configuration>
      <configuration
            TransformationFile="src/statem.ext"
            checkVisibility="true"
            contextId="org.eclipse.uml2.diagram.statemachine.ui.diagramContext"
            editorId="org.eclipse.uml2.diagram.statemachine.part.UMLDiagramEditorID">
         <transformations>
            <transformation
                  menu="true"
                  name="Create Composite"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="statemCreateComposite"
                  transformationId="de.cau.cs.kieler.uml2.statem.createComposite">
            </transformation>
         </transformations>
         <package
               class="org.eclipse.uml2.uml.UMLPackage">
         </package>
         <XtendTransformationFile
               TransformationFile="src/statem.ext">
         </XtendTransformationFile>
      </configuration>
      <configuration
            TransformationFile="src/usecase.ext"
            checkVisibility="true"
            contextId="org.eclipse.uml2.diagram.usecase.ui.diagramContext"
            editorId="org.eclipse.uml2.diagram.usecase.part.UMLDiagramEditorID">
         <transformations>
            <transformation
                  menu="true"
                  name="StartUp"
                  popup="false"
                  separated="false"
                  toolbar="false"
                  transformation="startUp"
                  transformationId="de.cau.cs.kieler.uml2.usecase.startUp">
            </transformation>
            <transformation
                  menu="true"
                  name="AddActorToUseCase"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addActorToUseCase"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addActorToUseCase">
            </transformation>
            <transformation
                  menu="true"
                  name="AddUseCasetoSubject"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addUseCasetoSubject"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addUseCasetoSubject">
            </transformation>
            <transformation
                  menu="true"
                  name="AddUseCaseToActor"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addUseCaseToActor"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addUseCaseToActor">
            </transformation>
            <transformation
                  menu="true"
                  name="AddActorGeneralization"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addActorGeneralization"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addActorGeneralization">
            </transformation>
            <transformation
                  menu="true"
                  name="AddUseCaseExtension"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addUseCaseExtension"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addUseCaseExtension">
            </transformation>
            <transformation
                  menu="true"
                  name="AddUseCaseGeneralization"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addUseCaseGeneralization"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addUseCaseGeneralization">
            </transformation>
            <transformation
                  menu="true"
                  name="AddUseCaseInclusion"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addUseCaseInclusion"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addUseCaseInclusion">
            </transformation>
            <transformation
                  menu="true"
                  name="InclusionToExtend"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="inclusionToExtend"
                  transformationId="de.cau.cs.kieler.uml2.usecase.inclusionToExtend">
            </transformation>
            <transformation
                  menu="true"
                  name="ExtendToInclusion"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="extendToInclusion"
                  transformationId="de.cau.cs.kieler.uml2.usecase.extendToInclusion">
            </transformation>
            <transformation
                  menu="true"
                  name="ExtendToGeneralization"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="extendToGeneralization"
                  transformationId="de.cau.cs.kieler.uml2.usecase.extendToGeneralization">
            </transformation>
            <transformation
                  menu="true"
                  name="IncludeToGeneralization"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="includeToGeneralization"
                  transformationId="de.cau.cs.kieler.uml2.usecase.includeToGeneralization">
            </transformation>
            <transformation
                  menu="true"
                  name="AddConstraintToExtension"
                  popup="false"
                  popupbar="false"
                  separated="false"
                  toolbar="false"
                  transformation="addConstraintToExtension"
                  transformationId="de.cau.cs.kieler.uml2.usecase.addConstraintToExtension">
            </transformation>
         </transformations>
         <package
               class="org.eclipse.uml2.uml.UMLPackage">
         </package>
         <XtendTransformationFile
               TransformationFile="src/usecase.ext">
         </XtendTransformationFile>
      </configuration>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.cau.cs.kieler.uml2.MetamodelRegistrator">
      </startup>
   </extension>
   
</plugin>
