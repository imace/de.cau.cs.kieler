<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cau.cs.kieler.kiml.service.graphTransformers">
      <handler
            class="de.cau.cs.kieler.kiml.formats.svg.SvgHandler"
            description="Scalable Vector Graphics (for export only)"
            extensions="svg"
            id="de.cau.cs.kieler.kiml.formats.svg"
            name="SVG">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="Test">
            <command
                  commandId="de.cau.cs.kieler.kiml.formats.svg.test"
                  label="Make SVG..."
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.cau.cs.kieler.kiml.formats.svg.Test"
            id="de.cau.cs.kieler.kiml.formats.svg.test"
            name="Test">
      </command>
   </extension>

</plugin>
