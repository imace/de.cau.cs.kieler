                <groups xsi:type="xtext:Keyword" value="t"/>
                <groups xsi:type="xtext:Keyword" value="n"/>
                <groups xsi:type="xtext:Keyword" value="f"/>
                <groups xsi:type="xtext:Keyword" value="r"/>
