<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://kieler.cs.cau.de/graphs"
            class="de.cau.cs.kieler.graphs.GraphsPackage"
            genModel="model/graphs.genmodel"/>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            canFinishEarly="false"
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.graphs.wizards.CreateRandomModelWizard"
            hasPages="true"
            icon="icons/obj16/random.gif"
            id="de.cau.cs.kieler.graphs.wizards.randomGraph"
            name="Create Random Graph"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpolicyProviders">
      <editpolicyProvider
            class="de.cau.cs.kieler.graphs.custom.GraphsDeletionPolicyProvider">
         <Priority
               name="Lowest">
         </Priority>
      </editpolicyProvider>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.graphs.wizards.ImportGraphWizard"
            icon="icons/importdiagram.gif"
            id="de.cau.cs.kieler.graphs.importWizard"
            name="Import Graph">
         <description>
            Import a graph from the local filesystem.
         </description>
      </wizard>
   </extension>
      
</plugin>
