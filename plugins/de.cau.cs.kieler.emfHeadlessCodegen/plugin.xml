<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="application.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="emfgen.application.application"
            name="emfgen">
         <property
               name="appName"
               value="emfgen">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            class="antTask.EMFCodeGenTask"
            headless="true"
            library="/"
            name="kieler.genEMFcode">
      </antTask>
   </extension>

</plugin>
