<?xml version="1.0" encoding="ASCII"?>
<xmi:XMI xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:xtext="http://www.eclipse.org/2008/Xtext">
  <xtext:Grammar name="de.cau.cs.kieler.ksbase.FeatureDefinition" usedGrammars="/1">
    <metamodelDeclarations xsi:type="xtext:GeneratedMetamodel" name="featureDefinition">
      <ePackage href="http://www.cau.de/cs/kieler/ksbase/FeatureDefinition#/"/>
    </metamodelDeclarations>
    <rules xsi:type="xtext:ParserRule" name="FeatureDefinitions">
      <type metamodel="/0/@metamodelDeclarations.0">
        <classifier xsi:type="ecore:EClass" href="http://www.cau.de/cs/kieler/ksbase/FeatureDefinition#//FeatureDefinitions"/>
      </type>
      <alternatives xsi:type="xtext:Group">
        <tokens xsi:type="xtext:Keyword" value="ModelPackage = "/>
        <tokens xsi:type="xtext:Assignment" feature="modelName" operator="=">
          <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
        </tokens>
        <tokens xsi:type="xtext:Keyword" value=";"/>
        <tokens xsi:type="xtext:Group" cardinality="?">
          <tokens xsi:type="xtext:Keyword" value="ModelPackagePath = "/>
          <tokens xsi:type="xtext:Assignment" feature="modelPath" operator="=">
            <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
          </tokens>
          <tokens xsi:type="xtext:Keyword" value=";"/>
        </tokens>
        <tokens xsi:type="xtext:Keyword" value="FeatureMenuTitle = "/>
        <tokens xsi:type="xtext:Assignment" feature="featureMenuTitle" operator="=">
          <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
        </tokens>
        <tokens xsi:type="xtext:Keyword" value=";"/>
        <tokens xsi:type="xtext:Group" cardinality="?">
          <tokens xsi:type="xtext:Keyword" value="TransformationFile = "/>
          <tokens xsi:type="xtext:Assignment" feature="featureFile" operator="=">
            <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
          </tokens>
          <tokens xsi:type="xtext:Keyword" value=";"/>
        </tokens>
        <tokens xsi:type="xtext:Assignment" cardinality="*" feature="elements" operator="+=">
          <terminal xsi:type="xtext:RuleCall" rule="/0/@rules.1"/>
