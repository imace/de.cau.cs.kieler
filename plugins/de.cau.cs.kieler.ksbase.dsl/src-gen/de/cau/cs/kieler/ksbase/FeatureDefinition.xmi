        <classifier xsi:type="ecore:EClass" href="http://www.cau.de/cs/kieler/ksbase/FeatureDefinition#//FeatureType"/>
      </type>
      <alternatives xsi:type="xtext:Group">
        <tokens xsi:type="xtext:Keyword" value="Feature"/>
        <tokens xsi:type="xtext:Assignment" feature="name" operator="=">
          <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.0"/>
        </tokens>
        <tokens xsi:type="xtext:Keyword" value="{"/>
        <tokens xsi:type="xtext:Group" cardinality="?">
          <tokens xsi:type="xtext:Keyword" value="File = "/>
          <tokens xsi:type="xtext:Assignment" feature="fileName" operator="=">
            <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
          </tokens>
          <tokens xsi:type="xtext:Keyword" value=";"/>
        </tokens>
        <tokens xsi:type="xtext:Keyword" value="MethodName = "/>
        <tokens xsi:type="xtext:Assignment" feature="methodName" operator="=">
          <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
