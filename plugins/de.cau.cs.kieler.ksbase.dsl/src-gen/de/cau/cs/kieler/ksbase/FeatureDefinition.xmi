          <tokens xsi:type="xtext:Assignment" feature="numParameter" operator="+=">
            <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.1"/>
          </tokens>
          <tokens xsi:type="xtext:Keyword" value=";"/>
        </tokens>
        <tokens xsi:type="xtext:Group" cardinality="+">
          <tokens xsi:type="xtext:Keyword" value="Parameter = "/>
          <tokens xsi:type="xtext:Assignment" feature="parameter" operator="+=">
            <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
          </tokens>
          <tokens xsi:type="xtext:Keyword" value=";"/>
        </tokens>
        <tokens xsi:type="xtext:Keyword" value="MenuEntry = "/>
        <tokens xsi:type="xtext:Assignment" feature="menuEntry" operator="=">
          <terminal xsi:type="xtext:RuleCall" rule="/1/@rules.2"/>
        </tokens>
        <tokens xsi:type="xtext:Keyword" value=";"/>
        <tokens xsi:type="xtext:Keyword" value="}"/>
