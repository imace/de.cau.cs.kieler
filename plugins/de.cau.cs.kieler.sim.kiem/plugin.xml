<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="de.cau.cs.kieler.sim.kiem.string.datacomponent" name="de.cau.cs.kieler.sim.kiem.string.datacomponent" schema="schema/de.cau.cs.kieler.sim.kiem.string.datacomponent.exsd"/>
   <extension-point id="de.cau.cs.kieler.sim.kiem.json.datacomponent" name="de.cau.cs.kieler.sim.kiem.json.datacomponent" schema="schema/de.cau.cs.kieler.sim.kiem.json.datacomponent.exsd"/>
