      <view
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.sim.kiem.ui.views.KiemView"
            icon="icons/kiemIcon.png"
            id="de.cau.cs.kieler.sim.kiem.view"
            name="Execution Manager"
            restorable="true">
