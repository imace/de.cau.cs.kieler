<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="analysisProviders" name="AnalysisProviders" schema="schema/analysisProviders.exsd"/>
   <extension-point id="resultVisualizers" name="ResultVisualizers" schema="schema/resultVisualizers.exsd"/>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="de.cau.cs.kieler"
            defaultHandler="de.cau.cs.kieler.kiml.grana.handlers.AnalysisHandler"
            id="de.cau.cs.kieler.kiml.grana.command.analyse"
            name="Analyze">
      </command>
      <command
            categoryId="de.cau.cs.kieler"
            defaultHandler="de.cau.cs.kieler.kiml.grana.handlers.ConfigureAnalysesHandler"
            id="de.cau.cs.kieler.kiml.grana.command.configureAnalyses"
            name="Configure Analyses">
      </command>
      <command
            categoryId="de.cau.cs.kieler"
            defaultHandler="de.cau.cs.kieler.kiml.grana.handlers.BatchHandler"
            id="de.cau.cs.kieler.kiml.grana.command.setupBatch"
            name="Setup Batch">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:de.cau.cs.kieler">
         <command
               commandId="de.cau.cs.kieler.kiml.grana.command.analyse"
               icon="icons/menu16/analyzediagram.gif"
               id="de.cau.cs.kieler.kiml.grana.menu.analysis"
               label="Analyze Graph"
               style="pulldown"
               tooltip="Analyze the Graph of the Current Diagram">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeEditor">
                  <test
                        forcePluginActivation="false"
                        property="de.cau.cs.kieler.kiml.ui.activeEditorIsSupported">
                  </test>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:de.cau.cs.kieler">
         <command
               commandId="de.cau.cs.kieler.kiml.grana.command.analyse"
               icon="icons/menu16/analyzediagram.gif"
               id="de.cau.cs.kieler.kiml.grana.menu.analysis"
               label="Analyze Graph"
               style="push"
               tooltip="Analyze the Graph of the Current Diagram">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeEditor">
                  <test
                        forcePluginActivation="false"
                        property="de.cau.cs.kieler.kiml.ui.activeEditorIsSupported">
                  </test>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="de.cau.cs.kieler.kiml.grana.command.setupBatch"
               label="Setup Analysis Batch..."
               style="push"
               tooltip="Analyze multiple graphs in batch mode">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="de.cau.cs.kieler.kiml.grana.canSetupBatch">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:de.cau.cs.kieler.kiml.grana.menu.analysis">
         <command
               commandId="de.cau.cs.kieler.kiml.grana.command.configureAnalyses"
               label="Configure..."
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.kiml.grana.ui.GranaGeneralPreferencePage"
            id="de.cau.cs.kieler.kiml.grana.general"
            name="Graph Analysis">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.kiml.grana.views.AnalysisResultViewPart"
            icon="icons/menu16/analyzediagramquick.gif"
            id="de.cau.cs.kieler.kiml.grana.views.analysisResults"
            name="Analysis Results"
            restorable="true">
      </view>
   </extension>
   <extension
         point="de.cau.cs.kieler.kiml.grana.analysisProviders">
      <category
            description="This is the default category."
            id="de.cau.cs.kieler.kiml.grana.defaultCategory"
            name="Other">
      </category>
      <category
            description="Basic graph analyses."
            id="de.cau.cs.kieler.kiml.grana.basic"
            name="Basic Analyses">
      </category>
      <category
            description="Analyses that analyse the graph drawing."
            id="de.cau.cs.kieler.kiml.grana.drawing"
            name="Drawing Analyses">
      </category>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.CompoundNodeCountAnalysis"
            description="The number of compound nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.compoundNodeCount"
            name="Compound Node Count">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.ConnectedComponentsAnalysis"
            description="The number of connected components, including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.connectedComponents"
            name="Connected Components">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeCountAnalysis"
            description="The number of edges including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.edgeCount"
            name="Edge Count">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.MultiEdgeCountAnalysis"
            description="The number of multi edges including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.multiEdgeCount"
            name="Multi Edge Count">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeCountAnalysis"
            description="The number of nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.nodeCount"
            name="Node Count">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeDegreeAnalysis"
            description="The minimum, average and maximum node degree of all nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.averageNodeDegree"
            name="Node Degree">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.CoverageAnalysis"
            description="Percentage of area covered with nodes and taken up by insets."
            id="de.cau.cs.kieler.kiml.grana.coverage"
            name="Coverage">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.nodeSize"
               weak="false">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.area"
               weak="false">
         </dependency>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.HyperNodeCountAnalysis"
            description="The number of hyper nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.hyperNodeCount"
            name="Hyper Node Count">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.AspectRatioAnalysis"
            description="The aspect ratio of the area a graph drawing occupies. (width / height)"
            id="de.cau.cs.kieler.kiml.grana.aspectRatio"
            name="Aspect Ratio">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.area"
               weak="false">
         </dependency>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.BiconnectedComponentsAnalysis"
            description="The number of biconnected components, including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.biconnectedComponents"
            name="Biconnected Components">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeDirectionAnalysis"
            description="The number of edges going in each of the four directions."
            id="de.cau.cs.kieler.kiml.grana.edgeDirections"
            name="Edge Directions">
         <component
               name="Top">
         </component>
         <component
               name="Left">
         </component>
         <component
               name="Bottom">
         </component>
         <component
               name="Right">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeLengthAnalysis"
            description="The minimum, average and maximum edge length."
            id="de.cau.cs.kieler.kiml.grana.edgeLength"
            name="Edge Length">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.BendsAnalysis"
            description="The number of bendpoints in the graph including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.bendpointCount"
            name="Number of Bends">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
         <component
               abbreviation="Sum"
               name="Sum">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeCrossingsAnalysis"
            description="The number of edge crossings, including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.edgeCrossings"
            name="Number of Edge Crossings">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
         <component
               abbreviation="Sum"
               name="Sum">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeEdgeOverlapsAnalysis"
            description="The number of overlaps between nodes and edges on all hierarchy levels."
            id="de.cau.cs.kieler.kiml.grana.nodeEdgeOverlaps"
            name="Number of Node-Edge Overlaps">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.SelfLoopAnalysis"
            description="The number of self loops."
            id="de.cau.cs.kieler.kiml.grana.selfLoops"
            name="Self Loops">
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeSizeAnalysis"
            description="The minimum, average and maximum node size."
            id="de.cau.cs.kieler.kiml.grana.nodeSize"
            name="Node Size">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
         <component
               name="Nodes">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.AreaAnalysis"
            description="The area a graph drawing occupies."
            id="de.cau.cs.kieler.kiml.grana.area"
            name="Area">
         <component
               name="Width">
         </component>
         <component
               name="Height">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.LayersAnalysis"
            description="Counts the number of horizontal and vertical layers in which the nodes are arranged."
            id="de.cau.cs.kieler.kiml.grana.layers"
            name="Number of Layers">
         <component
               abbreviation="Hor"
               name="Horizontal">
         </component>
         <component
               abbreviation="Ver"
               name="Vertical">
         </component>
      </provider>
      <provider
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.HyperedgeCrossingsAnalysis"
            description="The number of hyperedge crossings."
            id="de.cau.cs.kieler.kiml.grana.hyperedgeCrossings"
            name="Hyperedge Crossings">
      </provider>
   </extension>
   <extension
         point="de.cau.cs.kieler.kiml.grana.resultVisualizers">
      <type
            name="text">
      </type>
      <type
            name="html">
      </type>
      <type
            name="csv">
      </type>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.visualizers.ToStringVisualizer"
            priority="1"
            type="text">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.visualizers.ArrayResultVisualizer"
            priority="10"
            type="text">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.visualizers.AnalysisFailedHtmlVisualizer"
            priority="10"
            type="html">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.visualizers.BasicHtmlVisualizer"
            priority="1"
            type="html">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.visualizers.BasicCSVVisualizer"
            priority="1"
            type="csv">
      </visualizer>
      <visualizationMethod
            class="de.cau.cs.kieler.kiml.grana.ui.DialogVisualizationMethod"
            default="true"
            silent="false"
            type="html">
      </visualizationMethod>
      <visualizationMethod
            class="de.cau.cs.kieler.kiml.grana.ui.ViewVisualizationMethod"
            default="true"
            silent="true"
            type="html">
      </visualizationMethod>
   </extension>
   <extension
         point="de.cau.cs.kieler.core.kivi.combinations">
      <combination
            class="de.cau.cs.kieler.kiml.grana.handlers.LayoutAnalysisCombination"
            description="Perform automatic graph analysis after a layout has been calculated"
            name="Analysis after Layout">
      </combination>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="de.cau.cs.kieler.kiml.grana.canSetupBatch">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                 <instanceof
                     value="org.eclipse.core.resources.IFile">
                 </instanceof>
                 <instanceof
                     value="org.eclipse.core.resources.IFolder">
                 </instanceof>
                 <instanceof
                     value="org.eclipse.core.resources.IProject">
                 </instanceof>
               </or>
            </iterate>
         </with>
      </definition>
   </extension>

</plugin>
