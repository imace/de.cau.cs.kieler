<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="perspective"
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.cau.cs.kieler.krep.evalbench.ui.EvalBenchPerspective"
            icon="icons/logo16.gif"
            id="de.cau.cs.kieler.krep.evalbench.ui.perspective"
            name="KIELER (Evaluation Bench)">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.cau.cs.kieler.krep.evalbench.ui.editors.KasmEditor"
            contributorClass="org.eclipse.ui.part.EditorActionBarContributor"
            default="true"
            extensions="kasm"
            icon="icons/kiel_file.gif"
            id="de.cau.cs.kieler.krep.evalbench.ui.editors.kasm"
            name="KASM Editor">
      </editor>
      <editor
            class="de.cau.cs.kieler.krep.evalbench.ui.editors.KitEditor"
            contributorClass="org.eclipse.ui.part.EditorActionBarContributor"
            default="false"
            extensions="kit"
            icon="icons/kiel_file.gif"
            id="de.cau.cs.kieler.krep.evalbench.ui.editors.kit"
            name="KitEditor">
      </editor>
      <editor
            class="de.cau.cs.kieler.krep.evalbench.ui.editors.LstEditor"
            contributorClass="org.eclipse.ui.part.EditorActionBarContributor"
            default="true"
            extensions="lst"
            id="de.cau.cs.kieler.krep.evalbench.ui.editors.lst"
            name="LST Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.krep.evalbench.ui.views.ConnectionView"
            icon="icons/connection.gif"
            id="de.cau.cs.kieler.krep.evalbench.ui.views.connection"
            name="Connection">
      </view>
      <view
            allowMultiple="false"
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.krep.evalbench.ui.views.VerifyView"
            icon="icons/logo16.gif"
            id="de.cau.cs.kieler.krep.evalbench.ui.views.verify"
            name="Verify"
            restorable="true">
      </view>
      <view
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.krep.evalbench.ui.views.AssemblerView"
            icon="icons/krep.gif"
            id="de.cau.cs.kieler.krep.evalbench.ui.views.assembler"
            name="Assembler View"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.krep.evalbench.ui.EvalBenchPreferencePage"
            id="de.cau.cs.kieler.krep.evalbench.ui.preferences.evalbench"
            name="Evaluation Bench">
      </page>
   </extension>

</plugin>
