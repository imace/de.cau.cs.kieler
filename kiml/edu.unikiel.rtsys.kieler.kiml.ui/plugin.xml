         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="KIML.2OrMore">
         <and>
            <not>
               <count
                     value="?">
               </count>
            </not>
            <count
                  value="+">
            </count>
         </and>
      </definition>
      <definition
            id="KIML.activeMenuSelectionIs1">
         <with
               variable="activeMenuSelection">
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="KIML.activeMenuSelectionIs1OrMore">
         <with
               variable="activeMenuSelection">
            <count
                  value="+">
            </count>
         </with>
      </definition>
      <definition
            id="KIML.activeMenuSelectionIs2OrMore">
         <with
               variable="activeMenuSelection">
            <reference
                  definitionId="KIML.2OrMore">
            </reference>
         </with>
      </definition>
      <definition
            id="KIML.activeMenuSelectionIs2OrMoreShapeNodeEditParts">
         <with
               variable="activeMenuSelection">
            <and>
               <iterate
                     operator="or">
                  <adapt
                        type="org.eclipse.gmf.runtime.diagram.ui.editparts.ShapeNodeEditPart">
                  </adapt>
               </iterate>
               <reference
                     definitionId="KIML.2OrMore">
               </reference>
            </and>
         </with>
      </definition>
   </extension>

