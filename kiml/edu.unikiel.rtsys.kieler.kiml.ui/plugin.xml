<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="edu.unikiel.rtsys.kieler.kiml.ui.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="edu.unikiel.rtsys.kieler.kiml.ui.commands.category"
            id="edu.unikiel.rtsys.kieler.kiml.ui.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="edu.unikiel.rtsys.kieler.kiml.ui.commands.sampleCommand"
            class="edu.unikiel.rtsys.kieler.kiml.ui.handlers.KimlUiHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="edu.unikiel.rtsys.kieler.kiml.ui.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="edu.unikiel.rtsys.kieler.kiml.ui.menus.sampleMenu">
            <command
                  commandId="edu.unikiel.rtsys.kieler.kiml.ui.commands.sampleCommand"
                  mnemonic="S"
                  id="edu.unikiel.rtsys.kieler.kiml.ui.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="edu.unikiel.rtsys.kieler.kiml.ui.toolbars.sampleToolbar">
            <command
                  commandId="edu.unikiel.rtsys.kieler.kiml.ui.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="edu.unikiel.rtsys.kieler.kiml.ui.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.common.ui.services.action.contributionItemProviders">
      <contributionItemProvider
            checkPluginLoaded="false"
            class="edu.unikiel.rtsys.kieler.kiml.ui.KimlContributionItemProvider">
         <Priority
               name="Low">
         </Priority>
         <partContribution
               class="org.eclipse.gmf.runtime.diagram.ui.parts.IDiagramWorkbenchPart">
            <partAction
                  id="edu.unikiel.rtsys.kieler.kiml.ui.layoutAction01"
                  menubarPath="/diagramMenu/arrangeMenu/arrangeGroup"
                  toolbarPath="/arrangeMenu">
            </partAction>
            <partAction
                  id="edu.unikiel.rtsys.kieler.kiml.ui.testAction01"
                  menubarPath="/diagramMenu/arrangeMenu/arrangeGroup"
                  toolbarPath="/arrangeMenu">
            </partAction>
         </partContribution>
      </contributionItemProvider>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="edu.unikiel.rtsys.kieler.kiml.ui.WorkbenchPreferencePage"
            id="edu.unikiel.rtsys.kieler.kiml.ui.PreferencePage"
            name="KIML">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="edu.unikiel.rtsys.kieler.kiml.ui"
            name="KIELER">
      </category>
      <view
            category="edu.unikiel.rtsys.kieler.kiml.ui"
            class="edu.unikiel.rtsys.kieler.kiml.ui.views.KimlLayoutGraphView"
            icon="icons/sample.gif"
            id="edu.unikiel.rtsys.kieler.kiml.ui.views.KimlLayoutGraphView"
            name="KIML Layout Graph View">
      </view>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
