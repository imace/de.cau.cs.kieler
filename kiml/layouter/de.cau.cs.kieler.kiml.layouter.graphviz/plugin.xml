<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.cau.cs.kieler.kiml.layout.preferences.LayoutProvidersPreferencePage"
            class="de.cau.cs.kieler.kiml.layouter.graphviz.preferences.GraphvizPreferencePage"
            id="de.cau.cs.kieler.kiml.layouter.graphviz.preferences.GraphvizPreferencePage"
            name="GraphViz">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.cau.cs.kieler.kiml.layouter.graphviz.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="de.cau.cs.kieler.kiml.layout.kimlLayoutProvider">
      <kimlLayoutProvider
            class="kiel.layouter.graphviz.CircoLayoutProvider">
      </kimlLayoutProvider>
      <kimlLayoutProvider
            class="kiel.layouter.graphviz.DotLayoutProvider">
      </kimlLayoutProvider>
      <kimlLayoutProvider
            class="kiel.layouter.graphviz.NeatoLayoutProvider">
      </kimlLayoutProvider>
      <kimlLayoutProvider
            class="kiel.layouter.graphviz.TwopiLayoutProvider">
      </kimlLayoutProvider>
      <kimlLayoutProvider
            class="kiel.layouter.graphviz.FdpLayoutProvider">
      </kimlLayoutProvider>
   </extension>

</plugin>
