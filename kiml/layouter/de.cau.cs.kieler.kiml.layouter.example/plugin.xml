<extension
         point="de.cau.cs.kieler.kiml.layout.kimlLayoutProvider">
      <kimlLayoutProvider
            class="de.cau.cs.kieler.kiml.layouter.example.ExampleLayoutProvider">
      </kimlLayoutProvider>
