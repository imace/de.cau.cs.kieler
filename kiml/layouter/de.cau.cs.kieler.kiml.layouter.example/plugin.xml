            category="de.cau.cs.kieler.kiml.ui.preferences.LayoutProvidersPreferencePage"
            class="de.cau.cs.kieler.kiml.layouter.example.preferences.ExampleLayouterPreferencePage"
            id="de.cau.cs.kieler.kiml.layouter.example.preferences.ExampleLayouterPreferencePage"
