<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="edu.unikiel.rtsys.kieler.kiml.layout.preferences.LayoutProvidersPreferencePage"
            class="edu.unikiel.rtsys.kieler.kiml.layouter.example.preferences.ExampleLayouterPreferencePage"
            id="edu.unikiel.rtsys.kieler.kiml.layouter.example.preferences.ExampleLayouterPreferencePage"
            name="Example Layouter">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="edu.unikiel.rtsys.kieler.kiml.layouter.example.preferences.PreferenceInitializer">
      </initializer>
   </extension>
<extension
         point="edu.unikiel.rtsys.kieler.kiml.layout.kimlLayoutProvider">
      <kimlLayoutProvider
            class="edu.unikiel.rtsys.kieler.kiml.layouter.example.ExampleLayoutProvider">
         <Priority
               name="Low"></Priority>
      </kimlLayoutProvider>
   </extension>
</plugin>
